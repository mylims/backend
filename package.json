{
  "name": "backend",
  "version": "0.0.0",
  "description": "Backend for mylims project",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "maasencioh",
  "license": "MIT",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "eslint": "eslint src --ext ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run tsc",
    "test": "npm run test-coverage && npm run eslint",
    "test-coverage": "npm run test-only -- --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "start": "npm run tsc && node ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mylims/backend.git"
  },
  "bugs": {
    "url": "https://github.com/mylims/backend/issues"
  },
  "homepage": "https://github.com/mylims/backend#readme",
  "jest": {
    "preset": "./jest.config.js",
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "dependencies": {
    "apollo-server-fastify": "~2.12.0",
    "fastify": "^2.13.1",
    "graphql": "~15.0.0",
    "graphql-tools": "~5.0.0",
    "graphql-type-json": "~0.3.1",
    "lodash.merge": "~4.6.2",
    "mongodb": "~3.5.6"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "~1.1.5",
    "@types/graphql-type-json": "~0.3.2",
    "@types/jest": "~25.2.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/mongodb": "^3.5.8",
    "@typescript-eslint/eslint-plugin": "~2.28.0",
    "@typescript-eslint/parser": "~2.28.0",
    "eslint": "~6.8.0",
    "eslint-config-cheminfo": "~3.0.0",
    "eslint-config-cheminfo-typescript": "~5.0.0",
    "eslint-plugin-import": "~2.20.2",
    "eslint-plugin-jest": "~23.8.2",
    "eslint-plugin-prettier": "~3.1.3",
    "jest": "~25.3.0",
    "prettier": "~2.0.4",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.3.1",
    "typescript": "~3.8.3"
  }
}
